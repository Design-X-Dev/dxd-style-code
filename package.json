{
  "name": "dxd-style-code",
  "version": "0.1.12",
  "type": "module",
  "description": "DX Design System - Vue 3 UI Component Library",
  "main": "./dist/dxd-style-code.umd.cjs",
  "module": "./dist/dxd-style-code.js",
  "exports": {
    ".": {
      "import": "./dist/dxd-style-code.js",
      "require": "./dist/dxd-style-code.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./src/*": "./src/*",
    "./src/components": "./src/components/index.js"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook",
    "build-storybook:custom": "BASE_PATH=/file/dxd-style-code/ storybook build",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@heroicons/vue": "^2.2.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/vue3": "^8.4.0",
    "@storybook/vue3-vite": "^8.4.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "storybook": "^8.4.0",
    "tailwindcss": "^4.1.0",
    "vite": "^5.2.0",
    "vue": "^3.4.21"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui-library",
    "design-system",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Design-X-Dev/dxd-style-code.git"
  },
  "homepage": "https://github.com/Design-X-Dev/dxd-style-code#readme",
  "bugs": {
    "url": "https://github.com/Design-X-Dev/dxd-style-code/issues"
  },
  "dependencies": {
    "vue-draggable-next": "^2.3.0"
  }
}
